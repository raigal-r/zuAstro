{
  "name": "@pcd/pcd-collection",
  "version": "0.8.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "./src/*",
    "./dist/*",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build",
    "test": "ts-mocha --config ../../.mocharc.js --exit test/**/*.spec.ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@pcd/emitter": "0.2.0",
    "@pcd/passport-crypto": "0.8.0",
    "@pcd/pcd-types": "0.8.0",
    "@pcd/semaphore-identity-pcd": "0.8.0",
    "chai": "^4.3.7",
    "fast-json-stable-stringify": "^2.1.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/rsa-pcd": "0.3.0",
    "@pcd/tsconfig": "*",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.2",
    "chai-spies": "^1.0.0",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "node-rsa": "^1.1.1",
    "ts-mocha": "^10.0.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
